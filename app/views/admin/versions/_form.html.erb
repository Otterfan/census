<%= simple_form_for [:admin, @version] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :item_type %>
    <%= f.input :item_id %>
    <%= f.input :event %>
    <%= f.input :object %>
    <%= f.input :created_at %>
    <%= f.input :object_changes %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
